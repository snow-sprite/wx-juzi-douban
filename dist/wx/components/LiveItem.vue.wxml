          {{ value.content }}
        </view>
        <view class="_div data-v-14f97b3d zl-live__operate {{[  fontsize === 0 ? 'zl-mini-operate' : '',  fontsize === 1 ? 'zl-default-operate' : '',  fontsize === 2 ? 'zl-large-operate' : '']}}">
          <view class="_ul data-v-14f97b3d">
            <view class="_li data-v-14f97b3d">
              <image wx:if="{{value.up_counts > value.down_counts}}" src="/static/img/live/liveup.svg" class="_img data-v-14f97b3d {{[  fontsize === 0 ? 'zl-mini-up-img' : '',  fontsize === 1 ? 'zl-default-up-img' : '',  fontsize === 2 ? 'zl-large-up-img' : '']}}"></image>
              <image wx:if="{{value.up_counts < value.down_counts || value.up_counts == value.down_counts}}" src="/static/img/live/noliveup.svg" class="_img data-v-14f97b3d {{[  fontsize === 0 ? 'zl-mini-up-img' : '',  fontsize === 1 ? 'zl-default-up-img' : '',  fontsize === 2 ? 'zl-large-up-img' : '']}}"></image> <label class="_span data-v-14f97b3d zl-bull_num {{[  fontsize === 0 ? 'zl-mini-fontsize' : '',  fontsize === 1 ? 'zl-default-fontsize' : '',  fontsize === 2 ? 'zl-large-fontsize' : '',  isNightMode ? 'night-text' : '']}}">
                {{ value.up_counts }}
              </label>
            </view>
            <view class="_li data-v-14f97b3d">
              <image wx:if="{{value.up_counts < value.down_counts}}" src="/static/img/live/livedown.svg" class="_img data-v-14f97b3d {{[  fontsize === 0 ? 'zl-mini-down-img' : '',  fontsize === 1 ? 'zl-default-down-img' : '',  fontsize === 2 ? 'zl-large-down-img' : '']}}"></image>
