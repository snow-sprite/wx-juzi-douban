        <view class="_aside data-v-14f97b3d zl-live__publishtime {{[  fontsize === 0 ? 'zl-mini-fontsize' : '',  fontsize === 1 ? 'zl-default-fontsize' : '',  fontsize === 2 ? 'zl-large-fontsize' : '',  isNightMode ? 'night-text' : '']}}">
