<template name="f2289e70">
  <view class="_div data-v-00fec5f2 zl-detail">
    <view class="_h3 data-v-00fec5f2 zl-detail__title"><label class="_span data-v-00fec5f2 zl-detail__title--tag">
        {{ detailData.year }}.{{ detailData.month < 10 ? '0'+detailData.month : detailData.month }}.{{ detailData.day < 10 ? '0'+detailData.day : story.day  }}
      </label> | {{ detailData.title }}
    </view>
    <view class="_section data-v-00fec5f2 zl-detail__publish"></view>
    <view class="_div data-v-00fec5f2 zl-detail__thumb">
      <transition name="fade" class="_transition data-v-00fec5f2">
        <image src="{{detailData.picUrl}}" onerror="{{defaultThumb}}" wx:if="{{detailData.picUrl}}" class="_img data-v-00fec5f2"></image>
        <image src="/static/img/history/fail.png" wx:else class="_img data-v-00fec5f2"></image>
      </transition>
    </view>
    <rich-text class="_article data-v-00fec5f2 zl-detail__article" nodes="{{detailData.details}}"></rich-text>
  </view>
</template>